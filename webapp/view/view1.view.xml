        <ObjectListItem 
			title="{PRODUCT_ID}"
			intro="{NAME}"
			press=""
            icon="{PRODUCT_PIC_URL}"
			number="{PRICE}"
			numberUnit="{CURRENCY_CODE}"
            >        
			<firstStatus>
				<ObjectStatus
					text="{DESCRIPTION}"
					state="{path : 'status',
                            formatter:'.formatter.convertStatus'}"
                             />
			</firstStatus>
        </ObjectListItem>                    
