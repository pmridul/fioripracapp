<mvc:View xmlns:mvc="sap.ui.core.mvc" 
        xmlns="sap.m" 
        xmlns:t="sap.ui.table" 
        xmlns:core="sap.ui.core" 
        controllerName="CoreChange.ABAP.first.controller.view1">

    <Page title="View 1">
        <headerContent>
            <Button icon="sap-icon://action" press="onNext"></Button>
        </headerContent>
        
        <content>
            <VBox >
            <Label text="On Enter"></Label>
            <SearchField id="idSearch" search="onSearch"></SearchField>
            </VBox>

            <VBox >
            <Label text="Live Search"></Label>
            <SearchField id="idLiveSearch" liveChange="onLiveSearch" ></SearchField>
            </VBox>

           <!-- <List id="idList" items="{/fruits}" selectionChange="onItemSelect" mode="SingleSelectMaster" delete="onDelItem">-->
           <List id="idList" 
                 items="{/ProductSet}" 
                 selectionChange="onItemSelect" 
                 mode="SingleSelectMaster" 
                 delete="onDelItem"
                 growing="true"
                 growingThreshold="5"
                 >
             <headerToolbar>
                <Toolbar >
                    <ToolbarSpacer ></ToolbarSpacer>
                    <Button text="Delete" press="onDeleteItems" type="Reject" icon="sap-icon://delete"/>
                </Toolbar>
             </headerToolbar>
                <items>

